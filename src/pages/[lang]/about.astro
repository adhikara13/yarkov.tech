---
import { getLanguageKeys } from "@i18n/utils";
import { DEFAULT_LANGUAGE } from "@root/constants";
import AboutTemplate from "@templates/About.astro";

// TODO: do not generate pages without required data
export function getStaticPaths() {
	return getLanguageKeys()
		.filter((lang) => lang !== DEFAULT_LANGUAGE)
		.map((lang) => ({ params: { lang } }));
}
---

<AboutTemplate />
